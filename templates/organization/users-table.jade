extends user/users-table
- load i18n

block before_point_head
    if is_admin
        th.header

block before_point
    if is_admin
        td: form.kick-form(action=kick_url, method='POST')
            - csrf_token
            input(type='hidden', name='user', value=user.id)
            a.button(href='#') {% trans "Kick" %}
