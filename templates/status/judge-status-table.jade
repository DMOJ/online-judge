- load static from staticfiles
- load timedelta
- load i18n
- load list_processor
tr
    th {% trans "Judge" %}
    if see_all_judges
        th {% trans "Online" %}
    th {% trans "Uptime" %}
    th {% trans "Ping" %}
    th {% trans "Load" %}
    th {% trans "Runtimes" %}
for judge in judges
    tr
        td
            if perms.judge.change_judge
                a(href='{% url "admin:judge_judge_change" judge.id %}')= judge.name
            else
                = judge.name
        if see_all_judges
            td
                if judge.online
                    i.fa.fa-check-circle(style='color:#44AD41')
                else
                    i.fa.fa-minus-circle(style='color:#DE2121')
        td
            if judge.online
                | #{judge.uptime|timedelta:"simple"}
            else
                | {% trans "N/A" %}
        td.ping
            if judge.online and judge.ping_ms
                | #{judge.ping_ms|floatformat:3} ms
            else
                | {% trans "N/A" %}
        td
            if judge.online
                | #{judge.load|floatformat:3}
            else
                | {% trans "N/A" %}
        td
            if judge.online
                for key, info in judge.runtime_versions
                    - var runtime_versions = info['runtime']
                    a.runtime-label(href='javascript:void(0);',
                                    data-judge=judge.id, data-lang=key,
                                    data-runtime-info='{% include "runtime-version-fragment.jade" %}')= info.name
                    if not forloop.last
                        = ', '
            else
                | N/A
if not judges
    tr
        td(colspan='6')
            em {% trans "There are no judges available at this time." %}
