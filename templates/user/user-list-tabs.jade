- load i18n

.page-title: .tabs
    h2 {% trans title %}
    span.spacer
    ul
        li.tab(class=('active' if tab == 'list' else ''))
            a(href='{% url "user_list" %}') #[i.tab-icon.fa.fa-users] {% trans "Leaderboard" %}
        li.tab(class=('active' if tab == 'organizations' else ''))
            a(href='{% url "organization_list" %}') #[i.tab-icon.fa.fa-university] {% trans "Organizations" %}
